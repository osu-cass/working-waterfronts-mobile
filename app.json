{
    "name": "WorkingWaterfronts",

    "indexHtmlPath": "index.html",

    "framework": "touch",

    "url": null,

    "phonegap": {
        "config": {
            "name": "${app.name}",
            "id": "edu.oregonstate.cass.${app.name}",
            "verbose": true,
            "path": "${app.dir}/phonegap"
        }
    },

    "builds": {
      "android": {
          "packager": "phonegap",
          "phonegap" : {
              "config": {
                  "platform": "android"
              }
          }
      },

      "ios": {
          "packager": "phonegap",
          "phonegap" : {
              "config": {
                  "platform": "ios"
              }
          }
      }

    },

    "js": [
        {
            "path": "touch/sencha-touch.js",
            "x-bootstrap": true
        },
        {
            "path": "bootstrap.js",
            "x-bootstrap": true
        },
        {
            "path": "https://maps.googleapis.com/maps/api/js?key=AIzaSyA7nQlijpeKNz6sEl9wBPwGrEqHHf8Mt4c",
            "remote": true
        },
        {
            "path": "app.js",
            "bundle": true,
            "update": "delta"
        }
    ],

    "css": [
        {
            "path": "resources/css/app.css",
            "update": "delta"
        }
    ],

    "appCache": {
        "cache": [
            "index.html"
        ],
        "network": [
            "*"
        ],
        "fallback": []
    },

    "resources": [
        "resources/images",
        "resources/images/pins"
    ],

    "ignore": [
        "\.svn$"
    ],

    "archivePath": "archive",

    "requires": [
    ],

    "cache": {
        "enable": true,
        "deltas": "deltas"
    },

    "id": "de143476-f166-42a7-978e-a71096d2a24d"
}
